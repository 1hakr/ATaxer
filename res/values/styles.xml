<resources xmlns:android="http://schemas.android.com/apk/res/android">    <style name="ActionBar.Solid.Ataxer" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">        <item name="background">@drawable/ab_solid_ataxer</item>        <item name="backgroundSplit">@drawable/ab_bottom_solid_ataxer</item>        <item name="backgroundStacked">@drawable/ab_stacked_solid_ataxer</item>        <item name="progressBarStyle">@style/ProgressBar.Ataxer</item>    </style>    <style name="ActionBar.Transparent.Ataxer" parent="@style/Widget.AppCompat.ActionBar">        <item name="background">@drawable/ab_transparent_ataxer</item>        <item name="progressBarStyle">@style/ProgressBar.Ataxer</item>    </style>    <style name="PopupMenu.Ataxer" parent="@style/Widget.AppCompat.ListPopupWindow">        <item name="android:popupBackground">@drawable/menu_dropdown_panel_ataxer</item>    </style>    <style name="DropDownListView.Ataxer" parent="@style/Widget.AppCompat.ListView.DropDown">        <item name="android:listSelector">@drawable/selectable_background_ataxer</item>    </style>    <style name="ActionBarTabStyle.Ataxer" parent="@style/Widget.AppCompat.ActionBar.TabView">        <item name="android:textColor">@color/black</item>        <item name="android:background">@drawable/tab_indicator_ab_ataxer</item>    </style>    <style name="DropDownNav.Ataxer" parent="@style/Widget.AppCompat.Spinner.DropDown.ActionBar">        <item name="android:background">@drawable/spinner_background_ab_ataxer</item>        <item name="android:dropDownSelector">@drawable/selectable_background_ataxer</item>        <item name="android:popupBackground">@drawable/menu_dropdown_panel_ataxer</item>    </style>    <style name="ProgressBar.Ataxer" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">        <item name="android:progressDrawable">@drawable/progress_horizontal_ataxer</item>    </style>    <style name="ActionButton.CloseMode.Ataxer" parent="@style/Widget.AppCompat.ActionButton.CloseMode">        <item name="android:background">@drawable/btn_cab_done_ataxer</item>    </style>    <style name="Theme.Ataxer.Widget" parent="@style/Theme.AppCompat">        <item name="dropDownListViewStyle">@style/DropDownListView.Ataxer</item>        <item name="popupMenuStyle">@style/PopupMenu.Ataxer</item>    </style>    <style name="TextViewAppTheme">        <item name="android:textColor">#ff000000</item>        <item name="android:background">@drawable/text_background_ab_ataxer</item>    </style>    <style name="TextNumberAppTheme" parent="@android:style/Widget.TextView">        <item name="android:textColor">#ff000000</item>    </style>    <style name="Theme.Ataxer.BorderlessButton" parent="@style/Theme.Ataxer">        <item name="android:gravity">center_vertical</item>        <item name="android:background">@drawable/list_selector_holo_light</item>        <item name="android:padding">4dip</item>        <item name="android:minHeight">48dip</item>    </style>    <style name="MyTitleText">        <item name="android:textSize">20sp</item>        <item name="android:textColor">@color/blue</item>        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_marginBottom">5dip</item>    </style>    <style name="MyAnimationAbout">        <item name="android:windowEnterAnimation">@android:anim/slide_in_left</item>        <item name="android:windowExitAnimation">@anim/slide_out_left</item>    </style>    <style name="ProgressBar.AtaxerMultu" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">        <item name="android:progressDrawable">@drawable/progress_horizontal_multi</item>    </style>    <style name="Theme.Ataxer.ActionButton.Overflow" parent="@style/Widget.AppCompat.ActionButton.Overflow">        <item name="android:src">@drawable/ic_action_overflow</item>    </style>    <style name="Theme.Ataxer.SegmentedRadio" parent="@android:style/Widget.CompoundButton.RadioButton">        <item name="android:textSize">22sp</item>        <item name="android:textColor">@color/radio_text_colorlist</item>        <item name="android:gravity">center</item>        <item name="android:background">@drawable/btn_radio_segemented</item>        <item name="android:padding">5dip</item>        <item name="android:layout_width">0dip</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:button">@null</item>        <item name="android:minHeight">40dip</item>        <item name="android:stretchColumns">0</item>        <item name="android:layout_weight">1</item>    </style>    <style name="Theme.Ataxer.SegmentedRadioMini" parent="@android:style/Widget.CompoundButton.RadioButton">        <item name="android:textSize">22sp</item>        <item name="android:textColor">@color/radio_text_colorlist</item>        <item name="android:gravity">center</item>        <item name="android:background">@drawable/btn_radio_segemented</item>        <item name="android:padding">5dip</item>        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:button">@null</item>        <item name="android:minHeight">40dip</item>        <item name="android:stretchColumns">0</item>    </style>    <style name="Theme.Ataxer.DialogStyleLight" parent="@style/DialogStyleLight">        <item name="dialogBackground">@drawable/sdl_background_light</item>        <item name="showDivider">false</item>        <item name="titleTextColor">@color/sdl_title_text_light</item>        <item name="titleSeparatorColor">@color/sdl_title_separator_light</item>        <item name="messageTextColor">@color/sdl_message_text_light</item>        <item name="buttonTextColor">@color/sdl_button_text_light</item>        <item name="buttonSeparatorColor">@color/sdl_button_separator_light</item>        <item name="buttonBackgroundColorNormal">@color/sdl_button_normal_light</item>        <item name="buttonBackgroundColorPressed">@color/sdl_button_pressed_light</item>        <item name="buttonBackgroundColorFocused">@color/sdl_button_focused_light</item>    </style>    <style name="EditTextAppTheme" parent="@android:style/Widget.EditText">        <item name="android:textColor">#ff000000</item>        <item name="android:background">@drawable/edit_text_holo_light</item>    </style>    <style name="SpinnerAppTheme" parent="@android:style/Widget.Spinner">        <item name="android:background">@drawable/spinner_background_holo_light</item>        <item name="android:dropDownSelector">@drawable/list_selector_holo_light</item>    </style>    <style name="SpinnerAppTheme.DropDown" parent="@style/SpinnerAppTheme">        <item name="android:spinnerMode">dropdown</item>    </style>    <style name="ProgressBar.Large" parent="@android:Widget.ProgressBar.Large">        <item name="android:indeterminateDrawable">@drawable/progress_large_holo</item>    </style>    <style name="Theme.Ataxer.ListSeparator" parent="@style/Widget.Plus.TextView.Holo.ListSeparator.Light">        <item name="android:background">@drawable/list_section_divider_holo_light</item>        <item name="android:textColor">@color/blue</item>    </style>    <style name="PinButton" parent="@style/BorderlessButton">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">64dp</item>        <item name="android:layout_weight">1</item>        <item name="android:textSize">28sp</item>        <item name="android:textStyle">bold</item>        <item name="android:textColor">#666666</item>        <item name="android:fontFamily">sans-serif-condensed</item>    </style>    <style name="DialogEnterNoAnimation">        <item name="android:windowEnterAnimation">@anim/no_animation</item>    </style>    <style name="DialogExitNoAnimation">        <item name="android:windowExitAnimation">@anim/no_animation</item>    </style>    <style name="Title">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_marginBottom">8dp</item>        <item name="android:textSize">30sp</item>        <item name="android:textColor">@color/blue</item>        <item name="android:textStyle">bold</item>        <item name="android:fontFamily">sans-serif-light</item>    </style>    <style name="DailogTitle" parent="SDL.TextView.Title">        <item name="android:textSize">22sp</item>        <item name="android:layout_width">match_parent</item>        <item name="android:paddingLeft">@dimen/grid_4</item>        <item name="android:paddingRight">@dimen/grid_4</item>        <item name="android:paddingTop">@dimen/grid_4</item>        <item name="android:paddingBottom">@dimen/grid_4</item>        <item name="android:minWidth">400dp</item>    </style></resources>